<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<fx:root type="Pane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" spacing="268.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Choices" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Feature" />
         </children>
         <padding>
            <Insets bottom="3.0" left="20.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" layoutY="19.0">
         <children>
            <ListView fx:id="choices" />
            <VBox alignment="TOP_CENTER" spacing="8.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#add" text="Add" />
                  <Button mnemonicParsing="false" onAction="#remove" text="Remove" />
               </children>
               <padding>
                  <Insets top="15.0" />
               </padding>
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </VBox>
            <ListView fx:id="feature" />
         </children>
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" layoutX="525.0" layoutY="419.0">
         <children>
            <Button mnemonicParsing="false" onAction="#select" style="-fx-background-color: red;" text="Select" textFill="WHITE" />
         </children>
         <padding>
            <Insets bottom="10.0" top="5.0" />
         </padding>
      </HBox>
   </children>
</fx:root>
